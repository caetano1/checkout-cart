{
  "flex-layout.row#title": {
    "children": ["title"]
  },
  "flex-layout.row#product-list": {
    "children": ["product-list-wrapper"],
    "props": {
      "marginBottom": 7
    }
  },
  "product-list-wrapper": {
    "blocks": ["product-list"]
  },
  "flex-layout.row#shipping": {
    "children": ["shipping-wrapper"],
    "props": {
      "marginBottom": 8
    }
  },
  "shipping-wrapper": {
    "blocks": ["shipping-calculator"]
  },
  "flex-layout.row#summary": {
    "children": ["summary-wrapper"],
    "props": {
      "marginBottom": 3
    }
  },
  "summary-wrapper": {
    "blocks": ["checkout-summary"]
  },
  "divider#vertical": {
    "props": {
      "orientation": "vertical"
    }
  },
  "divider#horizontal": {
    "props": {
      "orientation": "horizontal"
    }
  },
  "flex-layout.row#go-to-checkout": {
    "children": ["go-to-checkout"],
    "props": {
      "blockClass": "goToCheckout",
      "paddingTop": 4,
      "paddingBottom": 4
    }
  }
}
